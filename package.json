{
	"name": "@ghostebony/svelte-heroicons",
	"version": "1.0.6",
	"type": "module",
	"author": "Pedro Am√©rico",
	"license": "MIT",
	"repository": "github:ghostebony/svelte-heroicons",
	"description": "Heroicons for Svelte",
	"keywords": [
		"svelte",
		"tailwind",
		"heroicons",
		"icons"
	],
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build:step:1": "node --loader ts-node/esm ./src/builder/icons.ts",
		"build:step:2": "svelte-package",
		"build:step:3": "node --loader ts-node/esm ./src/builder/types.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"packageManager": "pnpm@7.19.0",
	"engines": {
		"pnpm": "^7.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.9.3",
		"@sveltejs/package": "^2.0.0",
		"@types/node": "^18.14.5",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"heroicons": "2.0.13",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	}
}
